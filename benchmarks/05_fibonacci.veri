// Iterative Fibonacci calculation
fn fibonacci(n: int) -> int {
    let mut a: int = 0;
    let mut b: int = 1;
    let mut result: int = 0;

    if n == 0 {
        result = 0;
    } else {
        if n == 1 {
            result = 1;
        } else {
            for i in 2..n {
                let temp: int = a + b;
                a = b;
                b = temp;
            }
            result = a + b;
        }
    }
    result
}

fn main() -> int {
    fibonacci(8)
}
